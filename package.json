{
  "dependencies": {
    "debug": "^4.1.1",
    "github-rebase": "^0.4.0",
    "probot-serverless-now": "^0.2.1",
    "promise-retry": "^1.1.1",
    "shared-github-internals": "^0.2.0"
  },
