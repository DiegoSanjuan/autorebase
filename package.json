    "@types/node": "^10.12.18",
    "@types/promise-retry": "^1.1.3",
    "envalid": "^4.1.4",
    "is-base64": "^0.1.0",
    "jest": "^23.6.0",
    "jest-circus": "^23.6.0",
    "jest-junit": "^6.0.0",
    "prettier": "^1.15.3",
    "probot": "^7.4.0",
    "smee-client": "^1.0.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  },
  "engines": {
    "node": ">= 8.10.0"
  },
  "main": "app",
  "name": "autorebase",
